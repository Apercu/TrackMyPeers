{
	"name": "TrackMyPeers",
	"description": "Target your peers easily",
	"version": "1.5",
	"manifest_version": 2,
	"icons": {
		"16": "img/icon16.png",
		"24": "img/icon24.png",
		"48": "img/icon48.png",
		"64": "img/icon64.png",
		"128": "img/icon128.png"
	},
	"content_scripts": [{
		"matches": ["http://intra.42.fr/*", "https://intra.42.fr/*"],
		"js": ["js/lib/jquery-2.1.0.min.js", "js/content.js"],
		"css": ["css/style.css"]
	}],
	"permissions": [
		"https://dashboard.42.fr/"
	]
}
